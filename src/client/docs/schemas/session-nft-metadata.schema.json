{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SessionInfo",
  "type": "object",
  "required": [
    "archive_sha256",
    "archive_signature",
    "cost_donated_usd",
    "conjectures_attempted",
    "conjectures_proved",
    "prover",
    "prover_version",
    "proof_status",
    "public_key",
    "username"
  ],
  "properties": {
    "archive_sha256": {
      "type": "string"
    },
    "archive_signature": {
      "type": "string"
    },
    "cost_donated_usd": {
      "type": "number",
      "format": "double"
    },
    "conjectures_attempted": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "conjectures_proved": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "prover": {
      "type": "string"
    },
    "prover_version": {
      "type": "string"
    },
    "proof_status": {
      "type": "string"
    },
    "public_key": {
      "type": "string"
    },
    "username": {
      "type": "string"
    }
  }
}