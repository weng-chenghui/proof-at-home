{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ReviewInfo",
  "type": "object",
  "required": [
    "ai_comparison_cost_usd",
    "archive_sha256",
    "archive_signature",
    "packages_reviewed",
    "conjectures_compared",
    "recommendation",
    "review_id",
    "reviewed_session_ids",
    "reviewer_public_key",
    "reviewer_username",
    "top_prover"
  ],
  "properties": {
    "ai_comparison_cost_usd": {
      "type": "number",
      "format": "double"
    },
    "archive_sha256": {
      "type": "string"
    },
    "archive_signature": {
      "type": "string"
    },
    "packages_reviewed": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "conjectures_compared": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "recommendation": {
      "type": "string"
    },
    "review_id": {
      "type": "string"
    },
    "reviewed_session_ids": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "reviewer_public_key": {
      "type": "string"
    },
    "reviewer_username": {
      "type": "string"
    },
    "top_prover": {
      "type": "string"
    }
  }
}