{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ReviewSummary",
  "type": "object",
  "required": [
    "archive_sha256",
    "nft_metadata",
    "package_rankings",
    "packages_reviewed",
    "problems_compared",
    "recommendation",
    "review_id",
    "reviewer_username"
  ],
  "properties": {
    "archive_sha256": {
      "type": "string"
    },
    "nft_metadata": true,
    "package_rankings": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/PackageRankingSummary"
      }
    },
    "packages_reviewed": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "problems_compared": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "recommendation": {
      "type": "string"
    },
    "review_id": {
      "type": "string"
    },
    "reviewer_username": {
      "type": "string"
    }
  },
  "definitions": {
    "PackageRankingSummary": {
      "type": "object",
      "required": [
        "overall_score",
        "prover_session_id",
        "rank"
      ],
      "properties": {
        "overall_score": {
          "type": "number",
          "format": "double"
        },
        "prover_session_id": {
          "type": "string"
        },
        "rank": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0.0
        }
      }
    }
  }
}